#write a program to accept month number from user. display message "this month has 28 or 29 days" if month if february
number =int(input("enter month number "))

if number>0 and number<=12:
    if number==1:
        print("january",end=' ')
    elif number==2:
        print("february",end=' ')
        print("this month has 28 or 29 days")
    elif number==3:
        print("march",end=' ')
    elif number==4:
        print("april",end=' ')
    elif number==5:
        print("may",end=' ')
    elif number==6:
        print("june",end=' ')
    elif number==7:
        print("july",end=' ')
    elif number==8:
        print("august",end=' ')
    elif number==9:
        print("september",end=' ')
    elif number==10:
            print("october",end=' ')
    elif number==11:
            print("navember",end=' ')
    elif number==12:
            print("dicember",end=' ')
    else:
        print("invalid number")
