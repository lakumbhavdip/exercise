ex24
'''
write  a program to print Hollow inverted pyramid (half pyramid)

'''

row=6
while row>=1:
    column=1
    while column<=row:
        if row==6 or row==2 or row==1:
            print("* ",end='')
        elif column>=2 and column<row:
            print(" ",end=" ")
        else:
            print("*",end=" ")
        column=column+1
    print('')
    row=row-1
-------------------------------------------------------------------------------------------------------------------------------------------------------
ex25
'''
        *
       * *
      * * *
     * * * *
    * * * * *    

     '''
for row in range(5):
    for i in range(5-row-1):
       print(' ',end = ' ')
    for j in range (row + 1):
        print(" *",end="  ")

    print(' ')

for row in range(5):
    for i in range(5-row-1):
       print(' ',end = ' ')
    for j in range (2*row + 1):
        print(" *",end="")

    print(' ')
-------------------------------------------------------------------------------------------------------------------------------------------------------
ex26
'''
    * * * * *    
     * * * *
      * * *
       * *
        *

'''

for row in range (6):
    for sapace in range(row):
        print(" ",end ="")
    for astrik in range(6-row):
        print ("*",end = " ")
    print()

for row in range (6):
    for sapace in range(row):
        print(" ",end ="")
    for astrik in range(2*(6-row)-1):
        print ("*",end = "")
    print()
